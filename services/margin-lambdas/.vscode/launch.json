{
  "version": "0.2.0",
  "configurations": [
    {
      "name": "SESLambda",
      "type": "node",
      "request": "attach",
      "address": "localhost",
      "port": 5858,
      "localRoot": "${workspaceRoot}/.aws-sam/build/SESLambda",
      "remoteRoot": "/var/task",
      "protocol": "inspector",
      "stopOnEntry": false,
      "outFiles": [
        "${workspaceRoot}/.aws-sam/build/SESLambda/app.js"
      ],
      "sourceMaps": true
    },
    {
      "name": "CognitoPostConfirmLambda",
      "type": "node",
      "request": "attach",
      "address": "localhost",
      "port": 5858,
      "localRoot": "${workspaceRoot}/.aws-sam/build/CognitoPostConfirmLambda",
      "remoteRoot": "/var/task",
      "protocol": "inspector",
      "stopOnEntry": false,
      "outFiles": [
        "${workspaceRoot}/.aws-sam/build/CognitoPostConfirmLambda/app.js"
      ],
      "sourceMaps": true
    }
  ]
}